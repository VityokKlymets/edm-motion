{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "start": "NODE_ENV=production NODE_PATH=dist node dist",
    "build": "yarn tsc:compile",
    "dev": "NODE_ENV=development concurrently 'yarn:tsc:watch' 'yarn:nodemon:run'",
    "nodemon:run": "NODE_PATH=dist/ nodemon dist ",
    "tsc:watch": "tsc --project './tsconfig.json' --watch",
    "tsc:compile": "tsc --project './tsconfig.json'",
    "db:setup": "createdb emmotion",
    "migration": "yarn typeorm:db  migration:generate -n",
    "migrate": "yarn typeorm:db migration:run",
    "typeorm:db": "yarn db ./node_modules/typeorm/cli.js",
    "db": "NODE_PATH=src ts-node -P ./tsconfig.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@join-com/typeorm-class-validator-is-uniq": "^1.3.10",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.12.2",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "ffmetadata": "^1.6.0",
    "formidable": "^1.2.2",
    "get-audio-duration": "^2.0.3",
    "globby": "^11.0.2",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.2",
    "random-hash": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "typeorm": "^0.2.28",
    "uniqid": "^5.2.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.5",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.0.5"
  }
}
